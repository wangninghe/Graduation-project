
@{
    ViewBag.Title = "Eidt";
    Layout = "~/Views/Shared/_LayoutPage3.cshtml";
}
@using YAConpay.Models;
@model Site
<h2 style="text-align:center">网点信息修改</h2>
<form method="post" action="~/Site/Edit">
    <div>
        <input type="hidden" name="SiteId" value="@Model.SiteId" />
        <label> 名称：</label>
        <input type="text" name="SiteName" id="SiteName" value="@Model.SiteName" class="form-control" />
        <br />
        <label> 负责人：</label>
        <input type="text" name="SitePerson" id="SitePerson" value="@Model.SitePerson" class="form-control" />
        <br />
        <label> 联系方式：</label>
        <input type="text" name="SitePhone" id="SitePhone" value="@Model.sitePhone" class="form-control" />
        <br />
        <label> 地址：</label>
        <input type="text" name="SiteAddress" id="SiteAddress" value="@Model.SiteAddress" class="form-control" />
        <br />
        <label> 发货类别：</label>
        <input type="text" name="Site__Category" id="Site__Category" value="@Model.Site__Category" class="form-control" />
        <br />
        <label> 网点要求：</label>
        <input type="text" name="SiteDeta" id="SiteDeta" value="@Model.SiteDeta" class="form-control" />
        <br />
        <input type="submit" name="" value="修改" class="btn btn-lg btn-primary">
        <a class="btn btn-secondary" href="/Site/Index">返回</a>
    </div>
</form>